<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grad2"
    tools:context="com.example.izteeb.PMO.MedicineListview">

    <Button
        android:id="@+id/b61"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:background="@mipmap/arrow_back"
        android:padding="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:fontFamily="monospace"
        android:textColor="@color/white"
        android:textStyle="normal"
        android:text="View Medications."
        android:layout_alignBaseline="@id/b61"
        android:layout_toEndOf="@id/b61"
        android:layout_marginLeft="15dp"
        />

    <ListView
        android:layout_marginTop="15dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/list5"
        android:layout_below="@id/b61"
        >
    </ListView>

</RelativeLayout>
